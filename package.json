{
  "name": "wendealdashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "analyze:size": "node scripts/analyze-bundle.js size",
    "analyze:bundle": "node scripts/analyze-bundle.js analyze",
    "analyze": "npm run analyze:bundle",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:exports": "node scripts/check-exports.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:run": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathIgnorePatterns=integration --testPathIgnorePatterns=e2e",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --updateSnapshot",
    "preview": "vite preview",
    "webhook": "tsx src/services/notionWebhookServer.ts",
    "webhook:dev": "tsx watch src/services/notionWebhookServer.ts",
    "dev:with-webhook": "concurrently \"npm run dev\" \"npm run webhook\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/charts": "^2.6.2",
    "@ant-design/icons": "^6.0.0",
    "@novu/js": "^3.9.3",
    "@novu/react": "^3.9.3",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.85.5",
    "airtable": "^0.12.2",
    "antd": "^5.27.1",
    "axios": "^1.11.0",
    "deep-chat-react": "^2.2.2",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "iframe-resizer": "^5.5.5",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "minimatch": "^10.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-i18next": "^15.7.3",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "^30.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^4.21.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.10.5",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.91.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^4.0.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit --skipLibCheck"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
