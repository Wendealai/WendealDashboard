# 搴旂敤閰嶇疆
VITE_APP_TITLE=Wendeal Dashboard
VITE_APP_VERSION=1.0.0

# API閰嶇疆
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=10000

# WebSocket閰嶇疆
VITE_SOCKET_URL=http://localhost:3001
VITE_NOVU_APP_ID=your-novu-app-id
VITE_ENABLE_WEBSOCKET=false

# 璁よ瘉閰嶇疆
VITE_AUTH_REDIRECT_URL=http://localhost:3000/auth/callback
VITE_AUTH_LOGOUT_URL=http://localhost:3000/auth/logout

# 鑱婂ぉ閰嶇疆
VITE_N8N_WEBHOOK_URL=https://n8n.wendealai.com/webhook/wendealdashboardaichat
VITE_N8N_API_KEY=your-n8n-api-key-here

# Supabase锛堝憳宸?瀹㈡埛鎸佷箙鍖栵級
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# 寮€鍙戦厤缃?
VITE_DEV_MODE=true
VITE_DEBUG=true

# 鍔熻兘寮€鍏?
VITE_ENABLE_CHAT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# 鏂囦欢涓婁紶閰嶇疆
VITE_MAX_FILE_SIZE=10MB
VITE_ALLOWED_FILE_TYPES=pdf,jpg,jpeg,png,txt,doc,docx

# 涓婚閰嶇疆
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_SWITCH=true

# 鍥介檯鍖栭厤缃?
VITE_DEFAULT_LANGUAGE=zh-CN
VITE_SUPPORTED_LANGUAGES=zh-CN,en-US

# Doubao (Volcengine ARK) Vision API
VITE_DOUBAO_API_KEY=your-doubao-api-key-here

# Google Gemini Vision API (comma-separated keys for rotation)
VITE_GEMINI_API_KEYS=key1,key2,key3

# Google Maps API
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Google Calendar API (Dispatch sync)
VITE_GOOGLE_CALENDAR_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
VITE_GOOGLE_CALENDAR_ID=primary
